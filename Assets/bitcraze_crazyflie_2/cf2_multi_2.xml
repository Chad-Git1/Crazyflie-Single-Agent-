      <geom mesh="cf2_collision_8_drone_2" class="collision"/>
      <geom mesh="cf2_collision_9_drone_2" class="collision"/>
      <geom mesh="cf2_collision_10_drone_2" class="collision"/>
      <geom mesh="cf2_collision_11_drone_2" class="collision"/>
      <geom mesh="cf2_collision_12_drone_2" class="collision"/>
      <geom mesh="cf2_collision_13_drone_2" class="collision"/>
      <geom mesh="cf2_collision_14_drone_2" class="collision"/>
      <geom mesh="cf2_collision_15_drone_2" class="collision"/>
      <geom mesh="cf2_collision_16_drone_2" class="collision"/>
      <geom mesh="cf2_collision_17_drone_2" class="collision"/>
      <geom mesh="cf2_collision_18_drone_2" class="collision"/>
      <geom mesh="cf2_collision_19_drone_2" class="collision"/>
