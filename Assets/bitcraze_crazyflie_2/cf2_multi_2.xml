      <geom mesh="cf2_collision_21_drone_2" class="collision"/>
      <geom mesh="cf2_collision_22_drone_2" class="collision"/>
      <geom mesh="cf2_collision_23_drone_2" class="collision"/>
      <geom mesh="cf2_collision_24_drone_2" class="collision"/>
      <geom mesh="cf2_collision_25_drone_2" class="collision"/>
      <geom mesh="cf2_collision_26_drone_2" class="collision"/>
      <geom mesh="cf2_collision_27_drone_2" class="collision"/>
      <geom mesh="cf2_collision_28_drone_2" class="collision"/>
      <geom mesh="cf2_collision_29_drone_2" class="collision"/>
      <geom mesh="cf2_collision_30_drone_2" class="collision"/>
      <geom mesh="cf2_collision_31_drone_2" class="collision"/>
      <site name="imu_drone_2"/>
      <site name="actuation_drone_2"/>
    </body>
  </worldbody>

  <actuator>
    <motor class="cf2_drone_2" ctrlrange="0 0.35" gear="0 0 1 0 0 0" site="actuation_drone_2" name="body_thrust_drone_2"/>
    <motor class="cf2_drone_2" ctrlrange="-1 1" gear="0 0 0 -0.00001 0 0" site="actuation_drone_2" name="x_moment_drone_2"/>
    <motor class="cf2_drone_2" ctrlrange="-1 1" gear="0 0 0 0 -0.00001 0" site="actuation_drone_2" name="y_moment_drone_2"/>
    <motor class="cf2_drone_2" ctrlrange="-1 1" gear="0 0 0 0 0 -0.00001" site="actuation_drone_2" name="z_moment_drone_2"/>
  </actuator>

  <sensor>
    <gyro name="body_gyro_drone_2" site="imu_drone_2"/>
    <accelerometer name="body_linacc_drone_2" site="imu"/>
    <framequat name="body_quat_drone_2" objtype="site" objname="imu"/>
  </sensor>
</mujoco>
