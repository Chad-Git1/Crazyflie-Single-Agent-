<mujoco model="cf2">
  <compiler inertiafromgeom="false" meshdir="assets" autolimits="true"/>

  <default>
    <default class="cf2_drone_2">
      <default class="visual_drone_2">
        <geom group="2" type="mesh" contype="0" conaffinity="0"/>
      </default>
      <default class="collision_drone_2">
        <geom group="3" type="mesh"/>
      </default>
      <site group="5"/>
    </default>
  </default>

  <asset>
    <material name="polished_plastic_drone_2" rgba="0.631 0.659 0.678 1"/>
    <material name="polished_gold_drone_2" rgba="0.969 0.878 0.6 1"/>
    <material name="medium_gloss_plastic_drone_2" rgba="0.109 0.184 0.0 1"/>
    <material name="propeller_plastic_drone_2" rgba="0.792 0.820 0.933 1"/>
    <material name="white_drone_2" rgba="1 1 1 1"/>
    <material name="body_frame_plastic_drone_2" rgba="0.102 0.102 0.102 1"/>
    <material name="burnished_chrome_drone_2" rgba="0.898 0.898 0.898 1"/>

    <mesh name="cf2_0_drone_2" file="cf2_0.obj"/>
    <mesh name="cf2_1_drone_2" file="cf2_1.obj"/>
    <mesh name="cf2_2_drone_2" file="cf2_2.obj"/>
    <mesh name="cf2_3_drone_2" file="cf2_3.obj"/>
    <mesh name="cf2_4_drone_2" file="cf2_4.obj"/>
    <mesh name="cf2_5_drone_2" file="cf2_5.obj"/>
    <mesh name="cf2_6_drone_2" file="cf2_6.obj"/>
    <mesh name="cf2_collision_0_drone_2" file="cf2_collision_0.obj"/>
    <mesh name="cf2_collision_1_drone_2" file="cf2_collision_1.obj"/>
    <mesh name="cf2_collision_2_drone_2" file="cf2_collision_2.obj"/>
    <mesh name="cf2_collision_3_drone_2" file="cf2_collision_3.obj"/>
