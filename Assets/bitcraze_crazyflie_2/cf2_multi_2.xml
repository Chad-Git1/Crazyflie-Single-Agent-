    <mesh name="cf2_collision_14_drone_2" file="cf2_collision_14.obj"/>
    <mesh name="cf2_collision_15_drone_2" file="cf2_collision_15.obj"/>
    <mesh name="cf2_collision_16_drone_2" file="cf2_collision_16.obj"/>
    <mesh name="cf2_collision_17_drone_2" file="cf2_collision_17.obj"/>
    <mesh name="cf2_collision_18_drone_2" file="cf2_collision_18.obj"/>
