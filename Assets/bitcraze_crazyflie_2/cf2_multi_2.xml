    <mesh name="cf2_collision_31_drone_2" file="cf2_collision_31.obj"/>
  </asset>

  <worldbody>
    <!-- Slide x offset to allow space for other drone -->
    <body name="cf2_drone_2" pos="0.1 0 0.1" childclass="cf2_drone_2">
      <freejoint/>
      <inertial pos="0 0 0" mass="0.027" diaginertia="2.3951e-5 2.3951e-5 3.2347e-5"/>
      <!-- Drone 2: blue propellors -->
      <geom mesh="cf2_0_drone_2" rgba="0.1 0.1 0.6 1" class="visual"/>
      <geom mesh="cf2_1_drone_2" material="medium_gloss_plastic" class="visual"/>
      <geom mesh="cf2_2_drone_2" material="polished_gold" class="visual"/>
      <geom mesh="cf2_3_drone_2" material="polished_plastic" class="visual"/>
      <geom mesh="cf2_4_drone_2" material="burnished_chrome" class="visual"/>
      <geom mesh="cf2_5_drone_2" material="body_frame_plastic" class="visual"/>
      <geom mesh="cf2_6_drone_2" material="white" class="visual"/>
      <geom mesh="cf2_collision_0_drone_2" class="collision"/>
      <geom mesh="cf2_collision_1_drone_2" class="collision"/>
      <geom mesh="cf2_collision_2_drone_2" class="collision"/>
      <geom mesh="cf2_collision_3_drone_2" class="collision"/>
      <geom mesh="cf2_collision_4_drone_2" class="collision"/>
      <geom mesh="cf2_collision_5_drone_2" class="collision"/>
